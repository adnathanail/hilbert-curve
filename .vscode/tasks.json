{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Run hilbert_curve_3",
      "type": "shell",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "command": "${config:processing.path}",
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "dedicated"
      },
      "args": [
		"--force",
		"--sketch=${workspaceRoot}/hilbert_curve_3/",
		"--output=${workspaceRoot}/hilbert_curve_3/out",
		"--run"
	  ],
